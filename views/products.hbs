<table class="table">
  <thead>
    <tr>
      <th scope="col">Product</th>
      <th scope="col">Price</th>
      <th scope="col">ID</th>
      <th scope="col">Delete</th>
      <th scope="col">Add to card</th>
    </tr>
  </thead>
  <tbody>
    {{#each productss}}
      <tr>
        <td>{{name}}</td>
        <td>{{price}}$</td>
        <td>{{_id}}</td>
        <td><a href="/index/delete/{{_id}}">➖</a></td>
        <td><a href="/index/addToCard/{{_id}}">➕</a></td>
      </tr>
    {{/each}}
    <tr>
      <td><h4>TOTAL: ~{{floor}}$</h4></td>
    </tr>
  </tbody>
</table>